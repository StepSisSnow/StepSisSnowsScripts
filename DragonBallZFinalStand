local plrs = game:GetService("Players")
local TeamBased = false ; local teambasedswitch = "p"
local presskeytoaim = true; local aimkey = "v"
local raycast = false

local espupdatetime = 5; autoesp = false



local lockaim = true; local lockangle = 5




