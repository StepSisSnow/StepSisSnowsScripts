		end

	else
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
	end
end)
others:NewToggle("T.O.P. Respawn", "L to revive K to tp out", function(TOPRespawn)
    local lplr = game:GetService("Players").LocalPlayer
    local mouse = lplr:GetMouse()
    mouse.KeyDown:connect(function(key)
        if key == "k" then
            game:GetService("TweenService"):Create(lplr.Character.HumanoidRootPart, TweenInfo.new(0.3, Enum.EasingStyle.Linear), {CFrame = CFrame.new(100, 100, 100)}):Play()
        end
    end)
    mouse.KeyDown:connect(function(key)
        if key == "l" then
            local action = lplr.Character.SuperAction
            action:Destroy()
        end
    end)
end)
others:NewButton("No Flying Ki Drain", "Destroys ur Ki Drain whilst flying.", function()
    local metatable = getrawmetatable(game)
	local namecall = metatable.__namecall 
	setreadonly(metatable,false)
	
	metatable.__namecall = newcclosure(function(self,...)
		local method = getnamecallmethod()
			if method == "FireServer" and self.Name == "Super" then 
				return nil
			end 
		return namecall(self,...)
	end)
end)
