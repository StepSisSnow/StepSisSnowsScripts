local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/StepSisSnow/StepSisSnowsScripts/main/Insomniac.lua"))()
local Insomniac = lib.new("Insomniac 不眠症")
local Slow
local EGod
local FB
local Dusty
