StatChecker:addTextbox("Target Name", "", function(Target, focusLost)
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["2"].Button.Textbox.Text = game.Workspace.Live[Target].Race.Value 
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["3"].Button.Textbox.Text = game.Workspace.Live[Target].Stats["Health-Max"].Value
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["4"].Button.Textbox.Text = game.Workspace.Live[Target].Stats["Ki-Max"].Value
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["5"].Button.Textbox.Text = game.Workspace.Live[Target].Stats["Phys-Damage"].Value
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["6"].Button.Textbox.Text = game.Workspace.Live[Target].Stats["Ki-Damage"].Value
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["7"].Button.Textbox.Text = game.Workspace.Live[Target].Stats["Phys-Resist"].Value
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["8"].Button.Textbox.Text = game.Workspace.Live[Target].Stats["Ki-Resist"].Value
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["9"].Button.Textbox.Text = game.Workspace.Live[Target].Stats["Speed"].Value
    						if game:GetService("Workspace").Live[Target]:FindFirstChild("RebirthWings") then
    						    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["10"].Button.Textbox.Text = "Rebirth"
						elseif game:GetService("Workspace").Live[Target]:FindFirstChild("RealHalo") then
						game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["10"].Button.Textbox.Text = "Heaven"
						else
						        game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["10"].Button.Textbox.Text = "Pure"
						end
    game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container["2"].Button.Textbox.Text = ExtraThing
end)
