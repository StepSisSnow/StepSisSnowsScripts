statcheckthing = 1
for i, v in pairs(game.CoreGui["Insomniac 不眠症"].Main["Stat Checker"]["Stat Checker"].Container:GetDescendants()) do
if v:IsA("ImageButton") then
    v.Name = statcheckthing
