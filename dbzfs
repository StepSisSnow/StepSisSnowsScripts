local creditss = Insomniac:addPage("Credits", 5012544693)
local credits = creditss:addSection("Credits")
credits:addButton("Scripts made by Insomnia", function()
end)
credits:addButton("Motivation by my friends", function()
end)
local theme = Insomniac:addPage("Settings", 5012544693)
local colors = theme:addSection("Colors")
local Settings = theme:addSection("Settings")
Settings:addButton("Destroy UI", function()
	if game.CoreGui:FindFirstChild("Insomniac 不眠症") then
		game:GetService("CoreGui")["Insomniac 不眠症"]:Destroy();
		game.Workspace.UpdateFS:Destroy();
		game.Workspace.SnakeTrash:Destroy();
	end
end)
Settings:addKeybind("Toggle Keybind", Enum.KeyCode.F1, function()
    Insomniac:toggle()
end, function()
end)

Insomniac:SelectPage(Insomniac.pages[9], true)
